{
  "name": "booking-service",
  "version": "1.0.0",
  "description": "Simple Node.js Booking Service with TypeScript, Prisma, Express",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
